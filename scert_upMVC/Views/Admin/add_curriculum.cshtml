
@{
    ViewBag.Title = "add_curriculum";
    Layout = "~/Views/Shared/admin_mst.cshtml";
}

<div class="pcoded-content">
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="page-wrapper">

                <div class="page-body">
                    <!-- for change area -->
                    <h3 class="add_heading">Add Curriculum </h3>
                    <div class="page-wrapper" style="background-color: white;">


                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label">Select Course</label>
                            <div class="col-sm-10">
                                <select name="select" class="form-control">
                                    <option value="First Semester">First Semester</option>
                                    <option value="Second Semester">Second Semester</option>
                                    <option value="Third Semester">Third Semester</option>
                                    <option value="Fourth Semester">Fourth Semester</option>


                                </select>
                            </div>
                        </div>

                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label">Subject </label> <br>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" placeholder="Enter Subject...">
                            </div>
                        </div>


                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label">Upload Subject File</label> <br>
                            <div class="col-sm-10">
                                <input type="file" class="form-control">
                            </div>
                        </div>


                        <div class="row m-t-30">
                            <div class="col-md-12">
                                <button type="button"
                                        class="btn btn-primary btn-md btn-block waves-effect text-center m-b-20">
                                    Save
                                </button>
                            </div>
                        </div>

                    </div>
                    <!-- for change area end  -->
                </div>
            </div>

            <div id="styleSelector">

            </div>
        </div>
    </div>
</div>

