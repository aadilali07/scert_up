
@{
    ViewBag.Title = "add_Video";
    Layout = "~/Views/Shared/admin_mst.cshtml";
}

<div class="pcoded-content">
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="page-wrapper">

                <div class="page-body">
                    <!-- for change area -->
                    <h3 class="add_heading">Add Video </h3>
                    <div class="page-wrapper" style="background-color: white;">

                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label"> Video Title </label> <br>
                            <div class="col-sm-10">
                                <input type="text" class="form-control"
                                       placeholder="Enter Video title...">
                            </div>
                        </div>

                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label"> Video Description </label> <br>
                            <div class="col-sm-10">
                                <input type="text" class="form-control"
                                       placeholder="Enter Video description...">
                            </div>
                        </div>


                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label"> Video URL </label> <br>
                            <div class="col-sm-10">
                                <input type="text" class="form-control"
                                       placeholder="Enter Video URL...">
                            </div>
                        </div>





                        <div class="row m-t-30">
                            <div class="col-md-12">
                                <button type="button"
                                        class="btn btn-primary btn-md btn-block waves-effect text-center m-b-20">
                                    Save
                                </button>
                            </div>
                        </div>

                    </div>
                    <!-- for change area end  -->
                </div>
            </div>

            <div id="styleSelector">

            </div>
        </div>
    </div>
</div>

