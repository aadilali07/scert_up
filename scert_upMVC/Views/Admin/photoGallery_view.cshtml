
@{
    ViewBag.Title = "photoGallery_view";
    Layout = "~/Views/Shared/admin_mst.cshtml";
}

<div class="pcoded-content">
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="page-wrapper">
                <div class="page-body">
                    <!-- for change area -->
                    <h3 class="add_heading">List Of Images- </h3>

                    <div class="card">
                        <input type="text" id="myInput" onkeyup="myFunction()" placeholder="Search..." title="Type in a name">

                        <div class="card-block table-border-style">
                            <div class="table-responsive">
                                <table class="table" id="myTable">
                                    <thead>
                                        <tr>
                                            <th>Image Title </th>
                                            <th>Image Name</th>
                                            <th>Image</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <th scope="row">Img 1</th>
                                            <td>abc.jpg</td>
                                            <td>
                                                <img src="./assets/images/userlogo.png" width="60px"
                                                     height="60PX">
                                            </td>
                                            <td>
                                                <a href="#">Edit</a> |
                                                <a href="#">Delete</a>
                                            </td>
                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>


                    <!-- for change area  end-->
                </div>

            </div>

            <div id="styleSelector">

            </div>
        </div>
    </div>
</div>

