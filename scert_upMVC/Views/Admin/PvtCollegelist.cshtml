@model List<scert_upMVC.Models.PvtCollege_Models>
@{
    ViewBag.Title = "PvtCollegelist";
    Layout = "~/Views/Shared/admin_mst.cshtml";
}

<div class="pcoded-content">
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="page-wrapper">
                <div class="page-body">
                    <!-- for change area -->
                    <h3 class="add_heading">List Of Private College</h3>

                    <div class="card">
                        <input type="text" id="myInput" onkeyup="myFunction()" placeholder="Search..." title="Type in a name">

                        <div class="card-block table-border-style">
                            <div class="table-responsive">
                                <table class="table" id="myTable">
                                    <thead>
                                        <tr>
                                            <th>District</th>
                                            <th>Institute Name</th>
                                            <th>Number Of Seat</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <th scope="row">@item.District</th>
                                                <td> @item.Inst_Name </td>
                                                <td>@item.Seats</td>
                                                <td>
                                                    <a href="/admin/AddPvtCollege?id=@(item.Id)"><i class="fas fa-edit"></i></a>
                                                    <a href="/admin/DeletePvtCollege?id=@(item.Id)"><i class="fas fa-trash" style="color:red;"></i></a>
                                                </td>
                                            </tr>
                                        }
                                        </tbody>
                                </table>
                            </div>
                        </div>
                    </div>


                    <!-- for change area  end-->
                </div>

            </div>

            <div id="styleSelector">

            </div>
        </div>
    </div>
</div>

