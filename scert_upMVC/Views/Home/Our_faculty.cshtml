@model List<scert_upMVC.Models.manage_faculty>
@{
    ViewBag.Title = "Our_faculty";
}

<section class="inner-top">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-6 col-sm-12 col-xs-12">
                <h1 class="inn-title">OUR FACULTY</h1>
            </div>
        </div>
    </div>
</section>
<div class="clearfix"></div>
<section id="main-content" class="main-content innerpage">
    <div class="container" id="content">
        <div class="bar-camp">
            <div class="breed-crum-div">
                <ol class="breadcrumb">
                    <li><a href="index.html" class="none external">Home</a></li>
                    <li>About Us</li>
                    <li class="active">Our faculty</li>
                </ol>
            </div>
        </div>
        <!-- InstanceBeginEditable name="content-area" -->
        <div class="table-responsive">
            <table id="myTable" class="table table-bordered table-striped">
                <tr>
                    <th>S.NO.</th>
                    <th>Name</th>
                    <th>Designation </th>
                    <th>Joining Date </th>
                    <th>Mobile Number</th>
                    <th>Email ID </th>
                </tr>
                @{
                    var count = 1;
                }
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@count</td>
                        <td>@item.Name</td>
                        <td>@item.Designation</td>
                        <td>@item.JoiningDate</td>
                        <td>@item.MobileNo</td>
                        <td>@item.EmailId</td>
                    </tr>
                    count++;
                }

            </table>
        </div>
        <!-- InstanceEndEditable -->
    </div>
</section>
