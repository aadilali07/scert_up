@model List<scert_upMVC.Models.manage_compitition>
@{
    ViewBag.Title = "Competition";
}

<section class="inner-top">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-6 col-sm-12 col-xs-12">
                <h1 class="inn-title">Competitions</h1>
            </div>
        </div>
    </div>
</section>

<div class="clearfix"></div>

<section id="main-content" class="main-content innerpage">
    <div class="container" id="content">
        <div class="bar-camp">
            <div class="breed-crum-div">
                <ol class="breadcrumb">
                    <li><a href="index.html" class="none external">Home</a></li>
                    <li class="active">Competitions</li>
                </ol>
            </div>
        </div>
        <table class="table table-bordered table-striped">
            <tbody>
                <tr>
                    <th>
                        S.NO.
                    </th>
                    <th width="60%">
                        Competition Name
                    </th>
                    <th width="60%">
                        Comp. Date
                    </th>
                    <th width="10%" align="center">Action</th>
                </tr>
                @{
                    var count = 1;
                }
                @foreach (var item in Model)
                {
            <tr>
                <td>@count</td>
                <td>@item.Comp_Name</td>
                <td>@item.Comp_Date</td>
                <td><a href="@item.Comp_File_Path" target="_blank" class="btn btn-primary btn-xs">View</a></td>
            </tr>
                    count++;
                }

            </tbody>
        </table>


    </div>
</section>

