@model List<scert_upMVC.Models.Units_Models>
@{
    ViewBag.Title = "units";
}

<section class="inner-top">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-6 col-sm-12 col-xs-12">
                <h1 class="inn-title">Units</h1>
            </div>
        </div>
    </div>
</section>
<div class="clearfix"></div>

<section id="main-content" class="main-content innerpage">
    <div class="container" id="content">
        <div class="bar-camp">
            <div class="breed-crum-div">
                <ol class="breadcrumb">
                    <li><a href="index.html" class="none external">Home</a></li>
                    <li>Units</li>
                    <li class="active">Units</li>
                </ol>
            </div>
        </div>
        <table class="table table-bordered table-striped">
            <tbody>
                <tr>
                    <th>S.NO.</th>
                    <th>District</th>
                    <th>Institution Name </th>
                    <th>Address </th>
                    <th>Name of In-charge</th>
                    <th>Designation</th>
                    <th>Phone No.</th>
                    <th>Email </th>
                    <th>Website </th>
                </tr>
                @{
                    var count = 1;
                }
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@count</td>
                        <td>@item.District</td>
                        <td>@item.Inst_Name</td>
                        <td>@item.Address</td>
                        <td>@item.InchargeName</td>
                        <td>@item.Designation</td>
                        <td>@item.Phone</td>
                        <td>@item.Email</td>
                        <td><a href="@item.Website" target="_blank">@item.Website</a></td>
                    </tr>
                    count++;
                }
            </tbody>
        </table>
    </div>
</section>

