@model List<scert_upMVC.Models.manage_Go>
@{
    ViewBag.Title = "Govt_order";
}

<section class="inner-top">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-6 col-sm-12 col-xs-12">
                <h1 class="inn-title">Government Order</h1>
            </div>
        </div>
    </div>
</section>

<div class="clearfix"></div>

<section id="main-content" class="main-content innerpage">
    <div class="container" id="content">
        <div class="bar-camp">
            <div class="breed-crum-div">
                <ol class="breadcrumb">
                    <li><a href="index.html" class="none external">Home</a></li>
                    <li class="active">Government Order</li>
                </ol>
            </div>
        </div>
        <div class="research-pg">
            <table class="table table-bordered table-striped">
                <tbody>
                    <tr>
                        <th width="2%">S.NO.</th>
                        <th width="60%">Subject</th>
                        <th width="15%">Date</th>
                        <th width="15%">Go. No.</th>
                        <th width="10%" align="center">Action</th>
                    </tr>
                    @{
                        var count = 1;
                    }
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@count</td>
                            <td>@item.Subject</td>
                            <td>@item.date</td>
                            <td>@item.GONo</td>
                            <td><a href="@item.File_Path" target="_blank" class="btn btn-primary btn-xs">View</a></td>
                        </tr>
                        count++;
                    }

                </tbody>
            </table>
        </div>


    </div>
</section>

